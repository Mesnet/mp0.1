$('.material-tooltip').remove();
$('#task_<%=@task.id%>').replaceWith("<%= j render partial: 'tasks/task', locals: {task: @task} %>");
$('.tooltipped').tooltip();
<%- if @message && @message.present? %>
  M.toast({html: "<%= @message %>"})
<% end %>
<%- if @from_parent %>
  $('#linked_task_<%=@task.id%>').replaceWith("<%= j render partial: 'tasks/linked_task', locals: {task: @task} %>");
<% end %>